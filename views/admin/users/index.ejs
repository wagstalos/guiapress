<%- include('../../partials/header'); %>
    <%- include('../../partials/navbar'); %>

        <div class="container">
            <h2>Usuários</h2>
            <a href="/admin/users/create" class="btn btn-primary mb-4">Criar nova usuário</a>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Email</th>

                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(user=> { %>
                        <tr>
                            <td>
                                <%= user.id %>
                            </td>
                            <td>
                                <%= user.email %>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>



        <%- include('../../partials/footer') %>